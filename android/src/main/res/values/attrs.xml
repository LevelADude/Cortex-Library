<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="isMonogatariTheme" format="boolean" />
    <attr name="appThemeVariant" format="integer" />

    <declare-styleable name="TriStateButton">
        <attr name="android:text" />
        <attr name="button_checked" format="reference" />
        <attr name="button_unchecked" format="reference" />
        <attr name="button_ignored" format="reference" />
        <attr name="button_state" format="enum">
            <enum name="IGNORED" value="0" />
            <enum name="CHECKED" value="1" />
            <enum name="UNCHECKED" value="2" />
        </attr>
    </declare-styleable>
</resources>
